{
	"manifest_version": 3,
	"permissions": [
		"declarativeNetRequest",
		"declarativeNetRequestWithHostAccess",
		"activeTab",
		"webRequest"
	],
	"host_permissions": [
		"*://*.ecole-tres-directe.eu/*",
		"*://*.ecoledirecte.com/*",
		"*://api.ecoledirecte.com/mot-de-passe-oublie.awp",
		"*://localhost/*"
	],
	"background": {
		"scripts": [
			"service-worker.js"
		]
	},
	"declarative_net_request": {
		"rule_resources": [
			{
				"id": "change_origin",
				"enabled": true,
				"path": "rulesets/change_origin.json"
			}
		]
	},
	"action": {
		"default_title": "Ecole Tres Directe â€¢ Unblock",
		"default_popup": "popup/popup.html"
	},
	"browser_specific_settings": {
		"gecko": {
			"id": "etdu-firefox-self-host@ecole-tres-directe.eu",
			"update_url": "https://raw.githubusercontent.com/newtondotcom/Ecole-Tres-Directe-Unblock/main/updates.json",
			"data_collection_permissions": {
			"required": ["authenticationInfo", "websiteContent"],
			"optional": [],
			"none": false
			}
		}
	}
}