{
	"manifest_version": 3,
	"permissions": [
		"declarativeNetRequest",
		"declarativeNetRequestWithHostAccess",
		"activeTab",
		"webRequest"
	],
	"host_permissions": [
		"*://*.ecole-tres-directe.vercel.app/*",
		"*://*.ecoledirecte.com/*",
		"*://api.ecoledirecte.com/mot-de-passe-oublie.awp",
		"*://localhost/*"
	],
	"background": {
		"service_worker": "service-worker.js"
	},
	"declarative_net_request": {
		"rule_resources": [
			{
				"id": "change_origin",
				"enabled": true,
				"path": "rulesets/change_origin.json"
			}
		]
	},
	"action": {
		"default_title": "Ecole Tres Directe â€¢ Unblock",
		"default_popup": "popup/popup.html"
	}
}