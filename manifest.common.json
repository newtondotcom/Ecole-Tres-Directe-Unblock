{
	"name": "Ecole Tres Directe Unblock",
	"version": "0.3.1",
	"description": "Permet le fonctionnement du site Ecole Tres Directe sur PC. Fait la passerelle entre votre ordinateur et l'API d'EcoleDirecte.",
	"homepage_url": "https://ecole-tres-directe.vercel.app",
	"content_scripts": [
		{
			"matches": [
				"https://www.ecoledirecte.com/*"
			],
			"js": [
				"content-scripts/promoteEDP.js"
			]
		},
		{
			"matches": [
				"https://*.ecole-tres-directe.vercel.app/*",
				"http://localhost:3000/*"
			],
			"js": [
				"content-scripts/handlePostMessage.js",
				"content-scripts/removeEDPUnblockBanner.js"
			]
		},
		{
			"matches": [
				"*://api.ecoledirecte.com/mot-de-passe-oublie.awp"
			],
			"css": [
				"content-scripts/resetPassword.css"
			],
			"js": [
				"content-scripts/resetPassword.js"
			]
		}
	],
	"icons": {
		"48": "icons/EDP-Unblock-logo-dark.png"
	}
}